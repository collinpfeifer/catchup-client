{
  "expo": {
    "name": "catchup-client",
    "slug": "catchup-client",
    "version": "1.0.0",
    "orientation": "portrait",
    // "icon": "./assets/images/icon.png",
    "icon": "./assets/images/catchup-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/catch_up_logo-removebg-preview.png",
      "resizeMode": "contain",
      // "backgroundColor": "#ffffff"
      "backgroundColor": "#e8ebe8"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "Allow $(PRODUCT_NAME) to access your contacts."
      },
      "bundleIdentifier": "com.crpfeife.catchupclient"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cfd866e1-ac8c-4a20-b131-a906b61f0b7c"
      }
    }
  }
}
